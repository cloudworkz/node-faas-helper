{
  "name": "gcf-helper",
  "version": "0.7.0",
  "description": "Google Cloud Functions Helper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:google-cloud-tools/node-faas-helper.git",
  "author": "Christian Froehlingsdorf <chris@5cf.de>",
  "license": "SEE LICENSE IN ./LICENSE",
  "private": false,
  "scripts": {
    "tsv": "tsc -v",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist/ && node_modules/.bin/tsc -p tsconfig.json",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "fix": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix",
    "test:only": "APIFS_SECRET=123123123 PROJECT_ID=test_project _mocha -R spec --exit --timeout 1000 './test/**/*.js'",
    "test": "npm run build && npm run test:only"
  },
  "keywords": [
    "google",
    "cloud",
    "functions",
    "helper",
    "bigquery",
    "pubsub",
    "error",
    "faas",
    "framework",
    "bootstrap"
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^2.1.0",
    "@google-cloud/pubsub": "^0.28.1",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "~11.11.3",
    "@types/uuid": "~3.4.4",
    "express": "^4.16.4",
    "mocha": "^6.0.2",
    "tslint": "~5.14.0",
    "typescript": "~3.3.3333"
  }
}
